{
  "name": "vnm-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "cross-env NODE_ENV=development dotenv -e .env.development vue-cli-service serve",
    "serve:test": "cross-env NODE_ENV=development dotenv -e .env.test vue-cli-service serve",
    "serve:prod": "cross-env NODE_ENV=production dotenv -e .env.production vue-cli-service serve",
    "build:dev": "cross-env NODE_ENV=development  dotenv -e .env.development vue-cli-service build",
    "build:test": "cross-env NODE_ENV=development  dotenv -e .env.test vue-cli-service build",
    "build:staging": "cross-env NODE_ENV=production  dotenv -e .env.staging vue-cli-service build",
    "build:prod": "cross-env NODE_ENV=production  dotenv -e .env.production vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^3.4.0",
    "@fingerprintjs/fingerprintjs-pro-vue-v3": "1.4.0",
    "@vueuse/core": "^10.9.0",
    "animate.css": "^4.1.1",
    "aos": "^3.0.0-beta.6",
    "axios": "^1.3.2",
    "core-js": "^3.8.3",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.0.0",
    "element-plus": "^2.5.5",
    "fingerprintjs": "^0.5.3",
    "moment": "^2.29.4",
    "pinia": "^2.0.30",
    "sitemap-webpack-plugin": "^1.1.1",
    "swiper": "^11.0.7",
    "vue": "^3.4.19",
    "vue-i18n": "^9.10.1",
    "vue-qrcode-component": "^2.1.1",
    "vue-router": "4.3.0",
    "vue3-carousel": "^0.1.40",
    "vue3-marquee": "^3.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "moment-locales-webpack-plugin": "^1.2.0",
    "sass": "^1.58.0",
    "sass-loader": "^13.2.0",
    "terser-webpack-plugin": "^5.3.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
